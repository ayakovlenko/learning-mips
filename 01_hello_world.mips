.data

  out_string:
    .asciiz "Hello, World!\n"

.text

  main:

    # system call code for printing string = 4
    li $v0, 4
    # load address of string to be printed into $a0
    la $a0, out_string
    # call operating system to perform operation in $v0
    syscall

    # 17 is exit with error
    li $v0, 17
    # set error code to 1
    la $a0, 1
    # call operating system
    syscall
